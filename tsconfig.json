{
  "compilerOptions": {
    "target": "es2020",
    "removeComments": true,
    "outDir": "dist",
    "sourceMap": true,
    "alwaysStrict": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strict": true,
    "strictBindCallApply": true,
    "strictFunctionTypes": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "esModuleInterop": true,
    "module": "es2020",
  },
  "include": ["./src"]
}
